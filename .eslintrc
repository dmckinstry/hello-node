{
    "parser": "babel-eslint",
    "extends": "standard",
    "rules": {
      "no-unused-vars": 0,
      "max-len": [1, 120, 2, {"ignoreComments": true}],
      "prop-types": [0],
      "strict": 0,
      "comma-dangle": 0,
      "react/jsx-filename-extension": 0,
      "indent": ["error", 4],
      "semi": [2, "always"]
    }
  }